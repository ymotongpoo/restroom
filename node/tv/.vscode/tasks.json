{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "0.1.0",
    "command": "npm",
    "isShellCommand": true,
    "showOutput": "always",
    "suppressTaskName": true,
    "tasks": [
        {
            "taskName": "test",
            "args": [
                "run",
                "test"
            ]
        },
        {
            "taskName": "build",
            "args": [
                "run",
                "build"
            ]
        },
        {
            "taskName": "run",
            "args": [
                "run",
                "start"
            ]
        },
        {
            "taskName": "clean",
            "args": [
                "run",
                "cleane"
            ]
        },
        {
            "taskName": "install",
            "args": [
                "install"
            ]
        },
        {
            "taskName": "update",
            "args": [
                "update"
            ]
        }
    ]
}